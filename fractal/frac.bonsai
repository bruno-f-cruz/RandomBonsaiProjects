      <Expression xsi:type="Disable">
        <Builder xsi:type="PropertyMapping">
          <PropertyMappings>
            <Property Name="X" />
          </PropertyMappings>
        </Builder>
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="Combinator">
          <Combinator xsi:type="num:Cos" />
        </Builder>
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="PropertyMapping">
          <PropertyMappings>
            <Property Name="Y" />
          </PropertyMappings>
        </Builder>
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="Combinator">
          <Combinator xsi:type="gl:CreateVector2">
            <gl:X>0.862760663</gl:X>
            <gl:Y>-0.505612552</gl:Y>
          </Combinator>
        </Builder>
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="Combinator">
          <Combinator xsi:type="gl:UpdateUniform">
            <gl:UniformName>c</gl:UniformName>
            <gl:ShaderName>julia</gl:ShaderName>
          </Combinator>
        </Builder>
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="Combinator">
          <Combinator xsi:type="al:AudioCapture">
            <al:SampleRate>22050</al:SampleRate>
            <al:SampleFormat>Mono16</al:SampleFormat>
            <al:BufferLength>10</al:BufferLength>
          </Combinator>
        </Builder>
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="Combinator">
          <Combinator xsi:type="dsp:ConvertScale">
            <dsp:Depth>F32</dsp:Depth>
            <dsp:Scale>1</dsp:Scale>
            <dsp:Shift>0</dsp:Shift>
          </Combinator>
        </Builder>
