      </Expression>
      <Expression xsi:type="Combinator">
        <Combinator xsi:type="gl:UpdateUniform">
          <gl:UniformName>c</gl:UniformName>
          <gl:ShaderName>julia_set</gl:ShaderName>
        </Combinator>
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="Combinator">
          <Combinator xsi:type="rx:Timer">
            <rx:DueTime>PT0S</rx:DueTime>
            <rx:Period>PT0.01S</rx:Period>
          </Combinator>
        </Builder>
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="Combinator">
          <Combinator xsi:type="dsp:Rescale">
            <dsp:Min>0</dsp:Min>
            <dsp:Max>1</dsp:Max>
            <dsp:RangeMin>0</dsp:RangeMin>
            <dsp:RangeMax>0.1</dsp:RangeMax>
            <dsp:RescaleType>Linear</dsp:RescaleType>
          </Combinator>
        </Builder>
      </Expression>
      <Expression xsi:type="Disable">
        <Builder xsi:type="Combinator">
